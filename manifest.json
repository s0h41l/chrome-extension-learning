{
    "name": "Black Hole",
    "description": "Anni pa deyange",
    "version": "1.0",
    "permissions": ["contextMenus","activeTab"],
    "icons": {
        "16":"icons/16.png",
        "48":"icons/48.png",
        "64":"icons/64.png",
        "128":"icons/128.png"

    },
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["https://*/*", "http://*/*"],
            "js": ["index.js"]
        }
    ],
    "manifest_version": 2
}